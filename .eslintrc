{
  "parser": "babel-eslint",
  "extends": [
    "@react-native-community",
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-unresolved": [0],
    "import/no-extraneous-dependencies": ["error", {"packageDir": "./"}],
    "no-case-declarations": [0]
  }
}
